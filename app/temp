def firstCriteria(dataSet, labelColumn):
    number = dataset.loc[:, dataset.columns != labelColumn]
    minimumValuesInRows = dataset.min(axis=1)
    indexOfMaximumOfMinimum = minimumValuesInRows.idxmax()
    firstCriteriaValue = dataset.loc[indexOfMaximumOfMinimum, dataset.columns == labelColumn]
    print(firstCriteriaValue)
    return firstCriteriaValue

dataset = read_csv(json_config.dataSourceUrl)
print(firstCriteria(dataset, json_config.labelColumn))

